/* pages/mineInfo/mineInfo.wxss */
.container {
  width: 92%;
  height: 100%;
  margin: 0 auto;
}
.header {
  width: 130rpx;
  margin: 0 auto;
  margin-top: -25%;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position:right 100%;
  background-size: 38rpx 38rpx;
  
}
.header image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
}
.list {
  width: 100%;
  background-color: #ffffff;

}
.list .listitem {
  width: 100%;
  height: 69rpx;
  border-bottom: 2rpx RGBA(238, 238, 238, 1) solid;
  color: RGBA(50, 50, 50, 1);
  font-size: 28rpx;
  font-weight: Medium;
  font-family: PingFang SC;
  display: flex;
  flex-direction: row;
  padding-top: 31rpx;
  justify-content: space-between;
}
.list .listitem .listinput {
  width: 75%;
  margin-left: 35rpx;
  margin-top:-4rpx; 
  font-family: PingFang-SC-Bold;
  font-weight: Bold;
  color: #323232;
  font-size: 32rpx;
}
.list .listitem  .placeholder{
  color: #D4D4D4;
  font-size: 30rpx;
}
.picker {
  width: 100%;
  margin-left: -35%;
  margin-top:-4rpx; 
  font-family: PingFang-SC-Medium;
  font-weight: Medium;
  color: #323232;
  font-size: 32rpx;
}

.info_pic {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.info_pic view {
  width: 50%;
  display: flex;
  flex-direction: column;
  text-align: center;
}
.info_pic view image{
  width: 208rpx;
  height: 208rpx;
  margin: 0 auto;
  margin-top: 40rpx;
  margin-bottom: 22rpx;
}
.info_pic view text {
  font-size: 26rpx;
  color: #979CAF;
  font-weight: Medium;
  font-family: PingFang-SC-Medium;
}
.button_view {
  width: 100%;
  margin-top: 100rpx;
}
.button_view button {
  width: 630rpx;
  height: 98rpx;
  background-color: #DE1512;
  color: #FFFFFF;
  font-size: 32rpx;
  font-family: PingFang-SC-Medium;
  font-weight: Medium;
  margin: 26rpx auto;
  border-radius:40rpx;
}