.avatar-box {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.avatar-box .userinfo-avatar {
    width: 200rpx;
    height: 200rpx;
    margin: 20rpx;
    border: 1rpx solid #ebebeb;
    border-radius: 50%;
}

.avatar-box .userinfo-nickname {
    color: #8f8c8c;
}

.login-authorization-dialog-mask {
    position: fixed;
    width: 100%;
    height: 100%;
    background-color: #00000031;
    z-index: 2000;
}

.login-authorization-dialog-mask .login-authorization-dialog {
    box-sizing: border-box;
    text-align: center;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 600rpx;
    height: 400rpx;
    border-radius: 16rpx;
    background-color: #fff;
    padding-bottom: 130rpx;
}

.login-authorization-dialog-mask .login-authorization-dialog .message-box {
    padding: 20rpx;
    height: 100%;
    color: #363636;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.login-authorization-dialog-mask .login-authorization-dialog .button-area {
    display: flex;
    align-items: center;
    justify-content: space-around;
    height: 130rpx;
    width: 100%;
    border-top: 1rpx solid #ececec;
    border-bottom-left-radius: 16rpx;
    border-bottom-right-radius: 16rpx;
    background-color: #fff;
    position: absolute;
    bottom: 0;
    left: 0;
}

.login-authorization-dialog-mask .login-authorization-dialog .button-area .btn {
    height: 76rpx;
    border-radius: 10rpx;
    line-height: 76rpx;
    padding: 0 40rpx;
    background-color: #fff;
    border: 1rpx solid #ececec;
    color: #363636;
    -webkit-tap-highlight-color: transparent;
    font-size: 32rpx;
}

.login-authorization-dialog-mask .login-authorization-dialog .button-area .btn.confirm {
    color: #cc2144;
    box-sizing: content-box;
    margin: 0;
}

.search_btn_area {
    height: 90rpx;
    z-index: 100;
    background-color: #fff;
    width: 100%;
    box-sizing: border-box;
    padding: 0 35rpx;
    right: 0;
    padding-right: 190rpx;
}

.search_btn_area .search_input {
    position: relative;
    height: 75rpx;
    top: 50%;
    transform: translateY(-50%);
    width: 100%;
    background-color: #f0f0f0;
    border-radius: 50rpx;
    padding: 0 30rpx;
    padding-left: 80rpx;
    padding-right: 80rpx;
    line-height: 75rpx;
    font-size: 30rpx;
}

.search_btn_area .search_input::before {
    content: '';
    position: absolute;
    display: inline-block;
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    top: 50%;
    transform: translateY(-50%);
    left: 10rpx;
    background-position: center center;
    background-repeat: no-repeat;
    background-size: 95% 95%;
    background-color: #f0f0f0;
    background-image: url('data:image/png;base64,');
}

.container {
    padding: 0 30rpx;
}

.fuxin-intro {
    border-radius: 16rpx;
    background-color: #c4cbe4;
    padding: 30rpx;
    font-size: 40rpx;
    color: #fff;
}

.fuxin-intro .intro-item {
    padding-left: 38rpx;
}

.tag-title {
    margin-top: 20rpx;
    display: inline-block;
    position: relative;
    height: 40rpx;
    line-height: 40rpx;
    border-bottom: 6rpx solid #e75454;
    padding: 10rpx 0;
}

.fuxin-feat-title {
    margin-top: 20rpx;
}

.tag-title.team-info {
    border-bottom: 6rpx solid #e75454;
}

.feat-block {
    height: 100rpx;
    line-height: 100rpx;
    padding: 10rpx;
    margin: 20rpx 0;
    display: flex;
    align-items: center;
    border-radius: 8rpx;
    box-shadow: 0rpx 3rpx 10rpx 0rpx #d3d3d3;
}

.feat-block .desc {
    margin-left: 10rpx;
    flex-grow: 1;
}

.feat-block .try-app {
    color: #c9c9c9;
    font-size: 26rpx;
}

