page {
    padding-top: 78rpx;
}


/* start 头部搜索框 */

.header {
    margin-top: -78rpx;
    background-color: #fff;
    top: 0;
    height: 78rpx;
    width: 100%;
}

.search_input {
    position: relative;
    background-color: #F6F7FA;
    background-color: #f0f0f0;
    border-radius: 30rpx;
    width: 450rpx;
    height: 78rpx;
    margin-left: 25rpx;
    margin-right: 10rpx;
    padding-left: 78rpx;
}

.search_input::before {
    border-radius: 30rpx;
    position: absolute;
    left: 0;
    content: '';
    display: inline-block;
    width: 78rpx;
    height: 78rpx;
    background-color: #f0f0f0;
    background: url('data:image/png;base64,') no-repeat center center;
    background-size: 50%;
}

.placeholder {
    font-size: 32rpx;
    color: #979CAF;
    font-weight: Medium;
    font-family: PingFang-SC-Medium;
}

.cancel_btn {
    /* border-radius: 15rpx; */
    border: 0rpx solid #979CAF;
    background-color: #fff;
    padding: 0;
    margin-right: 25rpx;
    display: inline-block;
    height: 78rpx;
    width: 129rpx;
    font-size: 30rpx;
    line-height: 78rpx;
    position: absolute;
    top: 0;
    right: 0;
}


/* end 头部搜索框 */

.tab_box {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    font-size: 32rpx;
    font-weight: Medium;
    font-family: PingFang-SC-Medium;
    height: 100rpx;
}

.check {
    border-bottom: 6rpx solid #DE1512;
    color: #DE1512;
}

.tab_box>view {
    width: 40%;
    height: 100%;
    line-height: 100rpx;
    color: #000;
    text-align: center;
}

.tab_box_detail {
    padding: 0 35rpx;
}

.tab_box_detail>view {
    width: 100%;
    margin-top: 34rpx;
    display: flex;
    justify-content: space-around;
}

.left {
    width: 170rpx;
    height: 100%;
}

.left>image {
    width: 158rpx;
    height: 158rpx;
    margin: 0 auto;
}

.right {
    width: 73%;
    height: 100%;
    display: flex;
    justify-content: space-between;
}

.name {
    font-size: 30rpx;
    color: #323232;
    font-weight: Bold;
    font-family: PingFang-SC-Bold;
    margin-top: 20rpx;
}

.info {
    font-size: 28rpx;
    color: #7A7A7A;
    font-weight: Medium;
    font-family: PingFang-SC-Medium;
    margin-top: 10rpx;
}

.right image {
    width: 66rpx;
    height: 66rpx;
    margin-top: 50rpx;
}