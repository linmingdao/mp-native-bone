/* 九宫格 */

.sudoku {
    overflow: hidden;
}


/* 默认是屏幕三等分 */

.sudoku .sudoku_item {
    position: relative;
    box-sizing: border-box;
    width: 33.3333%;
    float: left;
    border: 1rpx solid #fff;
    padding-bottom: 33.3333%;
    overflow: hidden;
    height: 0;
}


/* 右上角删除按钮 */

.sudoku .sudoku_item .corner_delete_btn {
    position: absolute;
    right: 6rpx;
    top: 6rpx;
    background-color: #DE1512;
    display: inline-block;
    width: 50rpx;
    height: 50rpx;
    background-image: url('data:image/png;base64,');
    background-position: center center;
    background-repeat: no-repeat;
    background-size: 80% 80%;
    border-radius: 50%;
    z-index: 200;
}

.sudoku .sudoku_item .declare_text {
    position: absolute;
    bottom: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.7);
    display: inline-block;
    color: #fff;
    box-sizing: border-box;
    width: 100%;
    z-index: 200;
    height: 60rpx;
    font-size: 25rpx;
    line-height: 60rpx;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding: 0 25rpx;
}

.sudoku .sudoku_item.circle_bottom {
    border-bottom-right-radius: 50%;
    border-bottom-left-radius: 50%;
}

.sudoku .sudoku_item image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    border-radius: 6rpx;
}


/* 圆形图片 */

.sudoku .sudoku_item.circle image {
    border-radius: 50%;
}


/* 圆角矩形 */

.sudoku .sudoku_item.rounded_rectangle {
    border-radius: 15rpx;
}


/* 竖向间距 */

.sudoku .sudoku_item.vertical_spacing {
    margin-top: 15rpx;
}


/* 屏幕横向四等分，默认是三等分 */

.sudoku .sudoku_item.quartern {
    width: 25%;
    padding-bottom: 25%;
}


/* 五等分 */

.sudoku .sudoku_item.quinquesection {
    width: 20%;
    padding-bottom: 20%;
}


/* 添加按钮 */

.sudoku .sudoku_item.add_atlas {
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 50% 50%;
    background-color: #ececec;
}


/* 添加按钮 */

.sudoku .sudoku_item.share {
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 50% 50%;
    background-color: #ececec;
}

.sudoku .sudoku_item.add_atlas.circle,
.sudoku .sudoku_item.share.circle {
    border-radius: 50%;
}