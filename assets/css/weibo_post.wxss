/* 微博帖子样式 */

.wb_post {
    padding: 35rpx;
    padding-bottom: 0;
    background-color: #fff;
    margin-bottom: 20rpx;
}

.wb_post:last-child {
    margin-bottom: 0;
}


/* start 微博帖子头部 */

.wb_post .avatar_box {
    position: relative;
    padding-left: 100rpx;
    height: 100rpx;
}

.wb_post .avatar_box .avatar {
    position: absolute;
    left: 0rpx;
    border-radius: 50%;
    height: 100rpx;
    width: 100rpx;
    background-color: #d2d2d3;
}

.wb_post .avatar_box .outline {
    height: 100%;
    padding-left: 25rpx;
}

.wb_post .avatar_box .outline .title_text {
    height: 70rpx;
    line-height: 70rpx;
    font-size: 35rpx;
}

.wb_post .avatar_box .outline .outline_text {
    color: #c5c5c5;
    font-size: 30rpx;
    height: 30;
    line-height: 30rpx;
}

.wb_post .avatar_box .outline .outline_text label {
    float: right;
}


/* end 微博帖子头部 */


/* start 微博主体部分 */

.wb_post .main {
    padding: 30rpx 0;
}

.wb_post .main .title {
    font-size: 30rpx;
    font-weight: bold;
    color: #6568f8;
}

.wb_post .main .text {
    font-size: 30rpx;
}


/* 微博主体底部点赞按钮区域 */

.wb_post .footer {
    justify-content: space-around;
    display: flex;
    width: 100%;
    height: 100rpx;
    border-top: 1rpx solid #e9e9eb;
}

.wb_post .footer .opt_item {
    height: 100%;
    width: 100rpx;
    background-position: center center;
    background-repeat: no-repeat;
}

.wb_post .footer .opt_item.like {
    background-size: 74%;
    background-image: url('data:image/png;base64,');
}

.wb_post .footer .opt_item.comment {
    background-size: 58%;
    background-image: url('data:image/png;base64,');
}

.wb_post .footer .opt_item.forward {
    background-size: 51%;
    background-image: url('data:image/png;base64,');
}

.wb_post .footer .opt_item.share {
    background-size: 56%;
    background-image: url('data:image/png;base64,');
}

.wb_post .footer .opt_item.at {
    background-size: 56%;
    background-image: url('data:image/png;base64,');
}


/* end 微博主体部分 */


/* start 微博链接 */

.wb_post .link {
    border-radius: 5rpx;
    box-sizing: border-box;
    position: relative;
    background-color: #f5f5f5;
    height: 150rpx;
    padding: 20rpx;
    padding-left: 150rpx;
}

.wb_post .link .surface {
    position: absolute;
    left: 20rpx;
    width: 110rpx;
    height: 110rpx;
}

.wb_post .link .text {
    box-sizing: border-box;
    font-size: 30rpx;
    height: 110rpx;
    line-height: 110rpx;
    /* 文字超出部分隐藏省略号 */
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    /* display:-webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2; */
}